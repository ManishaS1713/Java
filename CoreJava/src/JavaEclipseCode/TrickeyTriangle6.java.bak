class  TrickeyTriangle6
{
	public static void main(String[] args) 
	{
		int n=10;
		for (int i=1; i<=n ;i++ )
		{
			for (int j=n-i; j>0 ;j-- )
			{
			if ( j==2 || j==i )
			{
				System.out.print("*");
			}
			else
				{
					System.out.print("");	
				}
			}
		System.out.println();
		}
	}
}
